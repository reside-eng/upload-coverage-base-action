{
  "name": "@reside-eng/upload-coverage-base-action",
  "version": "0.0.0-development",
  "description": "Custom github action for uploading base branch coverage on pull_request close event (instead of default branch push)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint . --ext .ts,.js",
    "build": "rimraf dist && ncc build src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.0",
    "coveralls-api": "1.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@side/commitlint-config": "1.0.1",
    "@side/eslint-config-base": "2.1.0",
    "@side/eslint-config-jest": "1.0.0",
    "@side/prettier-config": "1.0.0",
    "@types/jest": "29.5.12",
    "@types/node": "16.18.35",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "48.2.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reside-eng/upload-coverage-base-action.git"
  },
  "bugs": {
    "url": "https://github.com/reside-eng/upload-coverage-base-action/issues"
  },
  "author": {
    "name": "Side Engineering",
    "url": "https://github.com/reside-eng"
  },
  "prettier": "@side/prettier-config",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yaml,yml,graphql}": "prettier --write"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "packageManager": "yarn@4.1.0"
}
