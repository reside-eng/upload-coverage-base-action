{
  "name": "@reside-eng/upload-coverage-base-action",
  "version": "0.0.0-development",
  "description": "Custom github action for uploading base branch coverage on pull_request close event (instead of default branch push)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint . --ext .ts,.js",
    "build": "rimraf dist && ncc build src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/github": "5.1.1",
    "coveralls-api": "1.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
    "@side/commitlint-config": "0.2.0",
    "@side/eslint-config-base": "0.17.0",
    "@side/eslint-config-jest": "0.7.0",
    "@side/prettier-config": "0.1.0",
    "@tsconfig/node16": "1.0.3",
    "@types/jest": "29.5.0",
    "@types/node": "16.18.23",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsdoc": "40.1.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "rimraf": "4.4.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reside-eng/upload-coverage-base-action.git"
  },
  "bugs": {
    "url": "https://github.com/reside-eng/upload-coverage-base-action/issues"
  },
  "author": {
    "name": "Side Engineering",
    "url": "https://github.com/reside-eng"
  },
  "prettier": "@side/prettier-config",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yaml,yml,graphql}": "prettier --write"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
